<Window x:Class="WPFPlayground01.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFPlayground01"
        xmlns:vm="clr-namespace:WPFPlayground01.ViewModel;assembly=WPFPlayground01.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Window.DataContext>
        <vm:MenuVM/>
    </Window.DataContext>
    <Grid>
        <TextBlock x:Name="textBlock" TextWrapping="Wrap" Text="{Binding}"/>

    </Grid>
</Window>
